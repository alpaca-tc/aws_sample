[
  {
    "cpu": 1,
    "essential": true,
    "image": "${image_url}",
    "memory": 512,
    "name": "${container_name}",
    "portMappings": [
      {
        "containerPort": 3000,
        "hostPort": 3000
      }
    ],
    "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
            "awslogs-group": "${log_group_name}",
            "awslogs-region": "${log_group_region}"
        }
    }
  }
]
